{
  "name": "clock",
  "main": "./lib/clock",
  "version": "1.7.2",
  "description": "Display time in status bar",
  "keywords": [
    "clock",
    "time",
    "statusbar"
  ],
  "configSchema": {
    "seconds": {
      "title": "Show seconds",
      "type": "boolean",
      "description": "Toggle show seconds",
      "default": false
    },
    "format": {
      "title": "24-hour format",
      "type": "boolean",
      "description": "Toggle 12/24 time display format",
      "default": true
    },
    "am_pm_suffix": {
      "title": "AM/PM suffix",
      "type": "boolean",
      "description": "Show AM/PM suffix when 12-hour format is selected",
      "default": false
    },
    "icon": {
      "title": "Show icon",
      "type": "boolean",
      "description": "Show a clock icon",
      "default": false
    }
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "repository": "https://github.com/tong/atom-clock",
  "license": "MIT",
  "engines": {
    "atom": ">=1.6.0 <2.0.0"
  },
  "dependencies": {}
}
